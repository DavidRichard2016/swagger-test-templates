    headers: {'Content-Type': 'application/json'},
    body: {
    {{#each parameters}}
    {{this.name}}:'DATA GOES HERE',
    {{/each}}
    }
  }, function(error, res, body){
    {{#compare assertion 'expect'}}
    expect(body).to.have.property('name');
    {{/compare}}
