  it('should respond with {{description}}', function(done) {
      {{#validateResponse returnType noSchema}}
      /*eslint-disable*/
      {{> schema-partial this}}

      /*eslint-enable*/
      {{/validateResponse}}
      request({
        url: '{{path}}',
        {{#if queryParameters}}
        qs: {
        {{#each queryParameters}}
          {{this.name}}: 'DATA GOES HERE'{{#unless @last}},{{/unless}}
        {{/each}}
        },
        {{/if}}
        method: 'HEAD',
        headers: {
          'Content-Type': '{{contentType}}'{{#if headerParameters}},
          'Custom-Header': {
          {{#each headerParameters}}
          {{this.name}}: 'DATA GOES HERE'{{#unless @last}},{{/unless}}
          {{/each}} }{{/if}}
        }
      },
      function(error, res) {
        if (error) return done(error);

        {{#is assertion 'expect'}}
        expect(res.statusCode).to.equal({{responseCode}});
        {{/is}}
        {{#is assertion 'should'}}
        res.statusCode.should.equal({{responseCode}});
        {{/is}}
        {{#is assertion 'assert'}}
        assert.equal(res.statusCode, {{responseCode}});
        {{/is}}
        done();
      });
    });